<!--index.wxml-->
<van-nav-bar fixed title="月光宝盒" border="{{false}}" title-class="nav-title" custom-class="nav-bar" />
<view class="index-container">
  <van-image width="100%" height="200" src="/images/2.jpg" />
  <view class="nav-wrapper" wx:for="{{list}}" wx:key="name">
    <navigator url="{{item.url}}" open-type="navigate" class="nav-inner">
      <text>{{item.name}}</text>
    </navigator>
  </view>
</view>


.nav-bar {
  background: transparent !important;
}
.nav-title {
  color: #fff !important;
}
.index-container {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  background: #6bddf5;
}
.nav-wrapper {
  width: 600rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin: 0 auto 30rpx;
  text-align: center;
  background-color: #fff;
  border-radius: 8rpx;
  box-shadow: rgba(0, 0, 0, 0.3);
}
.nav-inner {
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

#mychart-dom-multi-bar {
  position: absolute;
  top: 0;
  bottom: 55%;
  left: 0;
  right: 0;
}

#mychart-dom-multi-line {
  position: absolute;
  top: 50%;
  bottom: 0;
  left: 0;
  right: 0;
}
